<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="background" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphic_user_interface.events.GUIEventController">
   <HBox alignment="CENTER_LEFT" layoutX="10.0" prefHeight="53.0" prefWidth="716.0"
         stylesheets="@../style.css">
      <Label fx:id="typeL" alignment="CENTER" minWidth="80.0" prefHeight="20.0" prefWidth="80.0" text="Type">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
         <font>
            <Font size="16.0"/>
         </font>
      </Label>
      <Label fx:id="numL" minWidth="80.0" prefWidth="80.0" text="Number &#10;of spots">
         <font>
            <Font size="16.0"/>
         </font>
         <HBox.margin>
            <Insets right="2.0"/>
         </HBox.margin>
      </Label>
      <Label alignment="CENTER" minWidth="80.0" prefWidth="80.0" text="Name">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
         <font>
            <Font size="16.0"/>
         </font>
      </Label>
      <Label fx:id="timeL" alignment="CENTER" prefHeight="20.0" prefWidth="110.0" text="Time">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
         <font>
            <Font size="16.0"/>
         </font>
      </Label>
      <Label fx:id="locationL" alignment="CENTER" minWidth="90.0" prefWidth="70.0" text="Location">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
         <font>
            <Font size="16.0"/>
         </font>
      </Label>
      <Label fx:id="speakerL" alignment="CENTER" minWidth="90.0" prefWidth="100.0" text="Speaker">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
         <font>
            <Font size="16.0"/>
         </font>
      </Label>
      <Label fx:id="remainingL" alignment="CENTER" contentDisplay="CENTER" text="Remaining &#10;spots&#10;">
         <font>
            <Font size="16.0"/>
         </font>
         <HBox.margin>
            <Insets/>
         </HBox.margin>
      </Label>
   </HBox>
   <ScrollPane fx:id="scPane" fitToHeight="true" fitToWidth="true" hbarPolicy="ALWAYS" layoutY="49.5" pannable="true"
               prefHeight="393.0" prefWidth="656.0" style="-fx-background-color: #353979#353979;"
               stylesheets="@../style.css" vbarPolicy="ALWAYS" AnchorPane.bottomAnchor="50.0"
               AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="60.0">
      <VBox fx:id="eventArea" minWidth="400.0" style="-fx-background-color: #353979#353979;"/>
   </ScrollPane>
   <HBox alignment="TOP_RIGHT" layoutY="459.0" prefHeight="33.0" prefWidth="715.0" stylesheets="@../style.css"
         AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="11.0">
      <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#updateClick"
              text="update">
         <HBox.margin>
            <Insets bottom="5.0" right="20.0"/>
         </HBox.margin>
      </Button>
   </HBox>
</AnchorPane>
